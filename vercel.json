{
     "version": 2,
     "builds": [
          {
               "src": "src/index.js",
               "use": "@vercel/node"
          }
     ],
     "routes": [
          { "src": "/api/auth/(.*)", "dest": "/src/index.js" },
          { "src": "/api/bugs/(.*)", "dest": "/src/index.js" },
          { "src": "/(.*)", "dest": "/src/index.js" }
     ]
}