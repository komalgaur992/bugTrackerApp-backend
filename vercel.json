{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "prisma": {
    "generator": "prisma-client-js",
    "output": "node_modules/.prisma/client",
    "push": true
  }
}